@model List<ERPDomain.Models.DynamicMenuModel>

@{
    ViewBag.Title = "LoadLevel2Menu";
}

<table class="table">
    
    @for (int i = 0; i < Model.Count; i++)
    {
        <tr>
            <td>
                @*@Html.ActionLink(Model[i].MenuName, Model[i].MethodName, Model[i].ControllerName, new { Area = Model[i].AreaName }, null)*@
                @Html.ActionLink(ResourcesPlugin.ResourceManager.GetString(Model[i].ResourceName), Model[i].MethodName, Model[i].ControllerName, new { Area = Model[i].AreaName }, null)
            </td>
            
        </tr>
    }

</table>




